{
    "version": "2.0.0",
    "options": {
        "env": {
            "PYTHONPATH": "${workspaceFolder}/src"
        },
    },
    "tasks": [
        {
            "args": [
                "/Zi",
                "/EHsc",
                "/Fe:",
                "${fileDirname}\\${fileBasenameNoExtension}.exe",
                "/I",
                "\"C:\\Program Files (x86)\\gurobi1202\\win64\\include\"",
                "/L",
                "\"C:\\Program Files (x86)\\gurobi1202\\win64\\lib\"",
                "${file}"
            ],
            "command": "cl.exe",
            "group": "build",
            "label": "cl.exe build active file",
            "problemMatcher": [
                "$msCompile"
            ],
            "type": "shell",
        },
        {
            "label": "Update Kardex Dataset",
            "command": "python",
            "args": [
                "${workspaceFolder}/src/kardex/generator.py",
                "-sp",
                "${workspaceFolder}/Data/kardex",
                "-lp", 
                "${workspaceFolder}/Data/kardex",
            ],
            "type": "shell",
            "group": {
                "kind": "build",
            },
            "presentation": {
                "reveal": "always",
                "panel": "new",
                "focus": true
            }
        },{
            
            "presentation": {
                "reveal": "always",
                "panel": "dedicated",
                "focus": true,
                "clear": true,
            },
            "label": "Regenerate cv for kardex",
            "command": "python",
            "args": [
                "${workspaceFolder}/src/trainers/kardex.py",
                "--save_path",
                "${workspaceFolder}/Data/kardex/results/json_3",
                "--load_path",
                "${workspaceFolder}/Data/kardex/crossvalidation/json_3/",
                "--data_path",
                "${workspaceFolder}/Data/kardex/",

            ],
            "type": "shell",
            "group": {
                "kind": "build",
            },
        },
        {
            "label": "Continue cv for kardex",
            "command": "python",
            "args": [
                "${workspaceFolder}/src/trainers/bcw.py",
                "--save_path",
                "${workspaceFolder}/Data/kardex/results/json_3",
                "--load_path",
                "${workspaceFolder}/Data/kardex/results/json_3",
                "--load"
            ],
            "type": "shell",
            "group": {
                "kind": "build",
            },
            "presentation": {
                "reveal": "always",
                "panel": "new",
                "focus": true
            }
        },
        {
            
            "presentation": {
                "reveal": "always",
                "panel": "dedicated",
                "focus": true,
                "clear": true,
            },
            "label": "Regenerate cv for Breast Cancer",
            "command": "python",
            "args": [
                "${workspaceFolder}/src/trainers/bcw.py",
                "--save_path",
                "${workspaceFolder}/Data/Breast Cancer Winsconsin (Diagnostic)/results/json_1",
                "--load_path",
                "${workspaceFolder}/Data/Breast Cancer Winsconsin (Diagnostic)/crossvalidation/json_1/",
            ],
            "type": "shell",
            "group": {
                "kind": "build",
            },
        },
        {
            "label": "Continue cv for Breast Cancer",
            "command": "python",
            "args": [
                "${workspaceFolder}/src/trainers/bcw.py",
                "--save_path",
                "${workspaceFolder}/Data/Breast Cancer Winsconsin (Diagnostic)/results/json_2",
                "--load_path",
                "${workspaceFolder}/Data/Breast Cancer Winsconsin (Diagnostic)/results/json_2",
                "--load"
            ],
            "type": "shell",
            "group": {
                "kind": "build",
            },
            "presentation": {
                "reveal": "always",
                "panel": "new",
                "focus": true
            }
        },
        {
            "label": "Generate BCW Gurobi Model",
            "command": "python",
            "args": [
                "${workspaceFolder}/src/gurobi/model.py",
                "--gen_model",
                "--save_path",
                "${workspaceFolder}/Data/Breast Cancer Winsconsin (Diagnostic)/results/gurobi",
                "--load_path",
                "${workspaceFolder}/Data/Breast Cancer Winsconsin (Diagnostic)/results",
                "--load_name",
                "bcw",
                "--case_index",
                "best",
            ],
            "type": "shell",
            "group": {
                "kind": "build",
            },
            "presentation": {
                "reveal": "always",
                "panel": "new",
                "focus": true
            }
        },
        {
            "label": "Generate BCW Gurobi Files",
            "command": "python",
            "args": [
                "${workspaceFolder}/src/gurobi/model.py",
                "--save_path",
                "${workspaceFolder}/Data/Breast Cancer Winsconsin (Diagnostic)/results/gurobi",
                "--load_path",
                "${workspaceFolder}/Data/Breast Cancer Winsconsin (Diagnostic)/results/models",
                "--save_name",
                "bcw",
                "--load_name",
                "bcw",
                "--case_index",
                "best",
            ],
            "type": "shell",
            "group": {
                "kind": "build",
            },
            "presentation": {
                "reveal": "always",
                "panel": "new",
                "focus": true
            }
        },
        {
            "label": "Meta testing py",
            "command": "python",
            "args": [
                "${workspaceFolder}/src/testing/meta.py"
            ],
            "type": "shell",
            "group": {
                "kind": "build",
            },
            "presentation": {
                "reveal": "always",
                "panel": "new",
                "focus": true
            }
        },
    ]
}